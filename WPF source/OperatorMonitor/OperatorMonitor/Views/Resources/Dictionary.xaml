<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="WhiteC">#EDF2F4</Color>
    <SolidColorBrush x:Key="WhiteB" Color="{StaticResource WhiteC}"/>

    <Color x:Key="GrayC">#A499B3</Color>
    <SolidColorBrush x:Key="GrayB" Color="{StaticResource WhiteC}"/>

    <Color x:Key="BlackC">#2B2D42</Color>
    <SolidColorBrush x:Key="BlackB" Color="{StaticResource WhiteC}"/>

    <Color x:Key="RedLC">#EF233C</Color>
    <SolidColorBrush x:Key="RedLB" Color="{StaticResource WhiteC}"/>
    
    <Color x:Key="RedDC">#D80032</Color>
    <SolidColorBrush x:Key="RedDB" Color="{StaticResource WhiteC}"/>
    
    

</ResourceDictionary>